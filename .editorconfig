root = true

[*]
end_of_line = crlf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = tab

[*.md]
trim_trailing_whitespace = false

[*.targets]
indent_style = tab
indent_size = 4

[*.cs]
indent_style = tab
indent_size = 4

